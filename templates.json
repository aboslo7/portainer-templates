[
  {
    "type": 1,
    "title": "Sonarr",
    "description": "Smart PVR for newsgroup and BitTorrent users.",
    "platform": "linux",
    "logo": "https://raw.githubusercontent.com/linuxserver/docker-templates/master/linuxserver.io/img/sonarr-icon.png",
    "image": "lscr.io/linuxserver/sonarr:latest",
    "ports": [
      {
        "container": 8989,
        "host": 8989
      }
    ],
    "volumes": [
      {
        "container": "/config",
        "bind": "/path/to/sonarr/config"
      },
      {
        "container": "/tv",
        "bind": "/path/to/tv"
      },
      {
        "container": "/downloads",
        "bind": "/path/to/downloads"
      }
    ],
    "env": [
      {
        "name": "PUID",
        "label": "PUID",
        "default": "1000"
      },
      {
        "name": "PGID",
        "label": "PGID",
        "default": "1000"
      },
      {
        "name": "TZ",
        "label": "TimeZone",
        "default": "Asia/Riyadh"
      }
    ]
  },
